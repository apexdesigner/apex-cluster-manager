{
  name: 'Apex Process Admin',
  chart: 'https://raw.githubusercontent.com/apexdesigner/apex-process-admin/main/helm-charts/apex-process-admin-0.0.5.tgz',
  parameters: [
	{
	  name: 'name',
	  type: 'Name',
	  default: 'apex-process-admin',
	  isRequired: true
	},
	{
	  name: 'camunda',
	  type: 'App',
	  templates: [
		'Camunda 8',
		'Camunda 8 Enterprise'
	  ],
	  isRequired: true
	},
	{
	  name: 'postgres',
	  type: 'App',
	  templates: [
		'Postgres'
	  ],
	  isRequired: true
	},
	{
	  name: 'adminEmails',
	  type: 'string',
	  default: '${appUser.email}',
	  isRequired: true
	},
	{
	  name: 'port',
	  type: 'Port',
	  isRequired: true
	}
  ],
  valuesTemplate: {
	appName: '${name}',
	service: {
	  nodePort: '${port}'
	},
	appConfig: [
	  {
		name: 'ZEEBE_ADDRESS',
		value: 'http://${camunda}-zeebe-gateway:26500'
	  },
	  {
		name: 'elasticsearchConfiguration',
		value: '{ "node": "http://elasticsearch-master-headless:9200" }'
	  },
	  {
		name: 'adminEmails',
		value: '${adminEmails}'
	  },
	  {
		name: 'PGHOST',
		value: '${postgres}-postgresql'
	  },
	  {
		name: 'PGPASSWORD',
		valueFrom: {
		  secretKeyRef: {
			name: '${postgres}-postgresql',
			key: 'postgres-password'
		  }
		}
	  }
	],
  },
  launchUrls: [
	{
	  label: 'Launch Apex Process Admin in a New Tab',
	  urlTemplate: 'http://localhost:${port}'
	}
  ]
}
